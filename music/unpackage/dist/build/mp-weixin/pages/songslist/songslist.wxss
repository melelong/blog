.songslist{height:auto;min-height:100vh;background-color:#fcfcfd;position:relative}.songslist .songslist_heard{width:100vw;height:30vh;background-color:pink;display:flex;justify-content:space-around;align-items:center}.songslist .songslist_heard .songslist_heard_img{width:244rpx;height:244rpx;border-radius:10px;border:1px solid silver;z-index:1}.songslist .songslist_heard .songslist_heard_text{width:300rpx;height:244rpx;color:#fff;font-size:34rpx;display:flex;align-items:center;z-index:1}.songslist .songslist_heard .Mask{position:absolute;top:0;width:100%;height:auto;height:30vh;background-color:rgba(0,0,0,.6);-webkit-filter:blur(10px);filter:blur(10px);z-index:0}.songslist .Song .Songs{border-bottom:1px solid rgba(0,0,0,.1);display:flex;align-items:center}.songslist .Song .Songs .Song_font{width:80vw;margin-left:12px}.songslist .Song .Songs .Song_font .name{width:100%;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;word-break:break-all}.songslist .Song .Songs .Song_font .info{width:100%;color:#888;font-size:28rpx;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;word-break:break-all}.songslist .Song .Songs .Song_font .info image{width:12px;height:8px;margin:0 4px 0 0}.songslist .Song .Songs .Song_img{width:14vw;height:57px;display:flex;justify-content:center;align-items:center}.songslist .Song .Songs .Song_img image{width:22px;height:22px}